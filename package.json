{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "http-server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/codermanoj2023/10-nodejs-typescript-project-with-tsup.git"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --format cjs --watch --onSuccess \"node dist/index.js\"",
    "lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\"",
    "start": "node dist/index.js",
    "test": "jest --detectOpenHandles",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "ts-node src/config/seed-reference-tables.ts",
    "deploy:export-prisma": "ts-node scripts/export-with-prisma.ts",
    "deploy:import-prisma": "ts-node scripts/import-with-prisma.ts",
    "deploy:neon": "scripts\\deploy-complete.bat",
    "deploy:export": "scripts\\export-local-db.bat",
    "deploy:schema": "scripts\\deploy-to-neon.bat",
    "deploy:import": "scripts\\import-to-neon.bat",
    "deploy:full": "scripts\\deploy.bat"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.5",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "tsup": "^8.5.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.10",
    "prisma": "^6.16.2",
    "typescript": "^5.8.3"
  }
}
